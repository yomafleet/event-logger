{
  "name": "yomafleet/event-logger",
  "description": "Log in YF's preference format",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Yomafleet\\EventLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "aungchitmin",
      "email": "aungchitmin@yomafleet.com"
    }
  ],
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "require": {
    "illuminate/support": "^9.34"
  },
  "require-dev": {
    "orchestra/testbench": "^7.9",
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yomafleet\\EventLogger\\ServiceProvider"
      ],
      "aliases": [
        "Yomafleet\\EventLogger\\EventLoggerFacade"
      ]
    }
  }
}
